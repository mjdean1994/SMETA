<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">

    <link rel="shortcut icon" href="assets/images/favicon_1.ico">

    <title>@ViewBag.Title | SMETA</title>

    @Styles.Render("~/bundles/ubold")
    @Styles.Render("~/bundles/fontawesome")
    
    
    <!-- HTML5 Shiv and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <![endif]-->

    <script src="http://d3js.org/d3.v3.js"></script>
    <script src="../../Scripts/d3Functions.js"></script>
    <script src="../../Scripts/site.js"></script>
</head>


<body class="widescreen fixed-left-void">

    <!-- Begin page -->
    <div id="wrapper" class="enlarged forced">

        <!-- Top Bar Start -->
        <div class="topbar">

            <!-- LOGO -->
            <div class="topbar-left">
                <div class="text-center">
                    <a href="@Url.Action("Index","Home")" class="logo"><i class="icon-chart icon-c-logo"></i><span>SMETA</span></a>
                </div>
            </div>

            <!-- Button mobile view to collapse sidebar menu -->
            <div class="navbar navbar-default" role="navigation">
                <div class="container">
                    <div class="">
                        <div class="pull-right visible-xs">
                            <button class="button-menu-mobile open-left waves-effect waves-light">
                                <i class="md md-menu"></i>
                            </button>
                            <span class="clearfix"></span>
                        </div>
                        <ul class="nav navbar-nav navbar-right pull-right">
                            <li class="hidden-xs">
                                <a href="@Url.Action("About", "Home")" class="waves-effect waves-light"><i class="icon-info"></i></a>
                            </li>
                        </ul>
                    </div>
                    <!--/.nav-collapse -->
                </div>
            </div>
        </div>
        <!-- Top Bar End -->
        
        @RenderBody()

    </div>
    <!-- END wrapper -->



    <script>
            var resizefunc = [];
    </script>

    @Scripts.Render("~/bundles/uboldjs")




</body>
</html>